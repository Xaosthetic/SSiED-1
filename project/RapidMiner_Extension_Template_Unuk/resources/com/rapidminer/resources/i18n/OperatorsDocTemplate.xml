<?xml version="1.0" encoding="windows-1252" standalone="no"?>
<operatorHelp lang="en_EN">

	<operator>
		<key>balancing_operator</key>
		<name>Balancing (Over/Undersampling)</name>
		<synopsis>Operator for balancing data.</synopsis>
		<help>This operator helps you balance your data. It uses random Over and Undersampling technique.</help>
		<inputPorts>
			<port name="example set input" type="com.rapidminer.example.ExampleSet">This input port expects an ExampleSet. This ExampleSet will be balanced.</port>
		</inputPorts>
	</operator>
	
	<operator>
  		<key>weighted_random_forest</key>
    	<name>Weighted random forest</name>
    	<synopsis>A random forest using the Gini criterion for discerning class weights.</synopsis>
    	<help>A random forest using Gini criterion for discerning class weights.</help>
	</operator>
	
